{
    "manifest_version": 2,
    "name": "Xenapse",
    "description": "Replace new tab page with a personal dashboard featuring time and inspiration.",
    "version": "1.1.1",
    "author": "Chandu",
   "browser_action": {
      "default_title": "Xenapse"
    },
   "chrome_url_overrides" : {
     "newtab": "newtab.html"
   },
    "permissions": ["activeTab","tabs"],
    "background": {
      "scripts": ["jquery-3.3.1.js","main.js"],
      "persistent": false
    }
}